#include <stdio.h>


int arr2[2][4] = {
    {10, 11, 12, 13},
    {14, 15, 16, 17}
};
    int main(void) {
      int i, j;
        for(i=0; i<2; i++) {
            printf("Address of %dth array = [%p] [%p]\n",  
                        i, *(arr2), *(arr2 + i)); 
          
        for (j = 0; j < 4; j++){
          printf("%d ", *(*(arr2 + i) + j));
        } 
        printf("\n");
        } 
    return 0;
    }